<script src="/static/js/twbs-pagination/jquery.twbsPagination.js"></script>
<script src="/static/js/mustache/mustache.js"></script>
<script src="/static/js/layer/layer.js"></script>
<script type="text/javascript">
var pageSize=8;
var keyword=$("#keyword").val();
var latestTime=$("#latestTime").val();
messareList(1,keyword,latestTime);
function messareList(page,keyword,latestTime){
	 $('#toHtml').html('');
	var htmls='';
		htmls +='<tr><th>编号</th><th class="t">问题</th>';
		htmls +='<th>提问人</th><th>提问日期</th>';
		htmls +='<th>查看回复</th></tr>';
	 $('#toHtml').append(htmls);	
		
        $.ajax({
            url:"/extranet/web/mhedu/xx!leavingMessareLists.action",
            data:{
                pageNo:page,
                pageSize:pageSize,
                keyword:keyword,
                latestTime:latestTime
            },
            dataType:"json",
            type:"post",
            success:function(resp){
                if(resp.state=='ok'){
                	var html='';
                    for(var i=0;i<resp.rows.length;i++){
                    	var e=i+1;
                    	html +='<tr><td>'+e+'</td>';
                    	html +='<td class="t"><a href="/extranet/web/mhedu/xx!LeavingMessareDetails.action?id='+resp.rows[i].id+'" class="ti">'+resp.rows[i].title+'</a></td>';
                    	html +='<td>'+resp.rows[i].name+'</td>';
                    	html +='<td><span class="date">'+resp.rows[i].questionDate+'</span></td>';
                    	if(resp.rows[i].replyState == 0){
                    		html +='<td><a href="#" class="blink">待回复</a></td>';
                    	}else{
                    		html +='<td><a href="/extranet/web/mhedu/xx!LeavingMessareDetails.action?id='+resp.rows[i].id+'" class="alink">已回复</a></td>';
                    	}
                    	html +='</tr>';
                    }
                    $('#toHtml').append(html);
                    $('#pageSize').text(pageSize);
                    $('#total').text(resp.total);
                    //重置分页组件否则保留上次查询的，一般来说很多问题出现与这三行代码有关如：虽然数据正确但是页码不对仍然为上一次查询出的一致
                    $('#pagination').empty();
                    $('#pagination').removeData("twbs-pagination");
                    $('#pagination').unbind("page");
                    //将页面的数据容器制空
                    //翻页设置
                    $('#pagination').twbsPagination({
                        first: false,
                        prev: "上一页",
                        next: "下一页",
                        last: false,
                        totalPages: resp.pageCount>0?resp.pageCount:1,
                        startPage:resp.pageIndex,
                        visiblePages: 7,
                        initiateStartPageClick:false,
                        paginationClass: 'pagination',
                        nextClass: 'next',
                        prevClass: 'prev',
                        lastClass: 'last',
                        firstClass: 'first',
                        pageClass: 'pageCell',
                        activeClass: 'cur',
                        disabledClass: 'disable',
                        
                        onPageClick: function (event, page) {
                            messareList(page,keyword,latestTime);
                        }
                    });
                }else{
                    layer.alert(resp.msg);
                }
            }
        });
    }
    
    //关键字查询
    function  toSearch(){
    	 keyword=$("#keyword").val();
    	 latestTime=$("#latestTime").val();
    	messareList(1,keyword,latestTime);
    }
    
    //日期查询
    function findDate(){
   		 keyword=$("#keyword").val();
    	latestTime=$("#latestTime").val();
    	messareList(1,keyword,latestTime);
    }
    
	
</script>
