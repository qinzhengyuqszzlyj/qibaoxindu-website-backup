    function reliclick() {
        // 创建地图实例
        var map = new BMap.Map("container", {enableMapClick: false});
        // 创建点坐标
        var point = new BMap.Point(121.236263, 31.118347);
        //地图默认展开的大小
        map.centerAndZoom(point, 12);
        //是否开启滚轮设置地图尺寸
        map.enableScrollWheelZoom(true);
        var data = [];
        var localSearch = new BMap.LocalSearch(map);

        var dataListValue = rlListValue;
        var title = "";
        // if(obj==1){
        //     dataListValue = bgListValue;
        //     // title = "曝光度：";
        // }else{
        //     dataListValue = cyListValue;
        //     // title = "参与度：";
        // }
        for (var i = 0; i < dataListValue.length; i++) {
            localSearch.search(dataListValue[i].name);
            localSearch.setSearchCompleteCallback(function (searchResult) {
                //获取经度和纬度，将结果显示在文本框中
                var poi = searchResult.getPoi(0);
                var dataValue = "";
                //循环遍历当前新闻属性信息
                for (var k = 0; k < dataListValue.length; k++) {
                    if (searchResult.keyword == dataListValue[k].name) {
                        dataValue = dataListValue[k];
                    }
                }
                data.push({
                    geometry: {
                        type: 'Point',
                        coordinates: [poi.point.lng, poi.point.lat]
                    },
                    count: dataValue.rlvalue
                });
                var dataSet = new mapv.DataSet(data);
                //热力图渐变变化
                var options = {
                    size: 20,
                    // gradient: {
                    // 0:"#ff09da"
                    //     0.1: "#97d509",
                    //     0.3: "#7bbf39",
                    //     0.6: "#ebad06",
                    //     0.9: "#ff830d",
                    //     1.0: "#ff3822",
                    // },
                    draw: 'heatmap'
                }
                new mapv.baiduMapLayer(map, dataSet, options);


                var myIcon = new BMap.Icon("/static/extranet/mhedu/zh/images/tm.png", new BMap.Size(20, 20), {
                    anchor: new BMap.Size(10, 10), //中心点设置
                });
                var marker = new BMap.Marker(new BMap.Point(poi.point.lng, poi.point.lat), {icon: myIcon});  // 创建标注
                map.addOverlay(marker); // 将标注添加到地图中
                // 创建信息窗口
                var opts = {
                    width: 300,
                    height: 100,
                    backgroundColor: 'blue',
                    title: searchResult.keyword
                };
                if (dataValue.rlvalue == 0) {
                    title = "";
                } else {
                    title = "热力值：" + dataValue.rlvalue;
                }
                var infoWindow = new BMap.InfoWindow(title, opts);
                // 点标记添加点击事件
                marker.addEventListener('click', function () {
                    map.openInfoWindow(infoWindow, new BMap.Point(poi.point.lng, poi.point.lat)); // 开启信息窗口
                });
            });
        }
    }


